{
  "name": "jest-dedupe-resolver",
  "homepage": "https://github.com/jaridmargolin/jest-dedupe-resolver",
  "description": "Custom Jest resolver to remove duplicate dependencies caused by `npm link`",
  "main": "lib/resolver.js",
  "scripts": {
    "codecov": "codecov",
    "format": "prettier-eslint --write \"**/*.js\"",
    "lint": "eslint lib test",
    "start": "jest --watch",
    "test": "nyc --reporter=lcov --reporter=text jest"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "jest-resolve": "^23.2.0",
    "read-pkg-up": "^4.0.0",
    "semver": "^5.5.0"
  },
  "devDependencies": {
    "codecov": "^3.0.2",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "1.0.0-rc.9",
    "jest": "^23.2.0",
    "lint-staged": "^7.2.0",
    "nyc": "^12.0.2",
    "prettier-eslint-cli": "^4.7.1"
  }
}
