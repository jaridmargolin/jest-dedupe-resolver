    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-promise": "4.0.0",
    "eslint-plugin-standard": "3.1.0",
    "husky": "1.0.0-rc.13",
    "jest": "23.5.0",
    "lint-staged": "7.2.0",
    "nyc": "13.0.1",
    "prettier-eslint-cli": "4.7.1"
  }
